{
    "hosting": {
        "cleanUrls": true,
        "headers": [
            {
                "headers": [
                    {
                        "key": "Strict-Transport-Security",
                        "value": "max-age=31536000; includeSubDomains; preload"
                    }
                ],
                "source": "*"
            }
        ],
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "public": "out",
        "trailingSlash": false
    }
}
